set -g status-justify "left"
set -g status "on"
set-option -g status-bg colour236
set-option -g status-fg colour254
set -g status-left "#[fg=colour235,bg=colour220,bold] #S #[fg=colour236,bg=colour236,nobold,nounderscore,noitalics]█"
set -g status-right "#[fg=colour235,bg=colour81] %Y-%m-%d  %H:%M #[fg=colour235,bg=colour220] #h "
setw -g window-status-format "#[fg=colour244,bg=colour236] #I #[fg=colour254,bg=colour236] #W "
setw -g window-status-current-format "#[fg=colour235,bg=colour81] #I #[fg=colour235,bg=colour81,bold] #W #[fg=colour81,bg=colour236,nobold,nounderscore,noitalics]"

